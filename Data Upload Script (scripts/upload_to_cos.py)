from ibm_boto3 import client
import os

cos = client(
    aws_access_key_id=os.getenv("COS_API_KEY"),
    aws_secret_access_key=os.getenv("COS_SECRET"),
    endpoint_url=os.getenv("COS_ENDPOINT")
)

# Upload soil data
cos.upload_file(
    Bucket="smart-farming-data",
    Key="soil.csv",
    Filename="../datasets/soil_data.csv"
)
